---
import Layout from '@layouts/Layout.astro';
import { getStaticPathsWrapper } from '@util/getStaticPaths';
import CTA from '@components/cta/index.astro';
import FAQ from '@components/faq/index.astro';
import UAEFAQ from '@components/faq/uae.astro';

export const getStaticPaths = getStaticPathsWrapper(['pak', 'are'], {
  pak: ['en', 'ur'],
  are: ['en', 'ar'],
});

const { country } = Astro.params;
---

<Layout>
  {country === 'are' ? <UAEFAQ /> : <FAQ />}
  <CTA />
</Layout>
